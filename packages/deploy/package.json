{
  "name": "@airnode/deploy",
  "version": "0.1.0",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "index": "node .build/index.js",
    "sls:deploy:aws": "sls deploy --config serverless.aws.yml",
    "sls:invoke:aws": "sls invoke --function start --config serverless.aws.yml",
    "sls:remove:aws": "sls remove --config serverless.aws.yml",
    "sls:test:aws": "sls invoke --function test --config serverless.aws.yml"
  },
  "dependencies": {
    "ethers": "^5.0.17",
    "serverless-plugin-typescript": "^1.1.9"
  }
}
