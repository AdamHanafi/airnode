{
  "name": "@airnode/deployer",
  "license": "MIT",
  "version": "0.1.0",
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "deploy": "node dist/deployer-cli.js deploy -c config.json -s security.json",
    "deploy:local": "test -f .env && export $(egrep -v '^#' .env | xargs) && yarn deploy",
    "help": "node dist/deployer-cli.js --help",
    "remove-airnode": "node dist/deployer-cli.js remove-airnode -c config.json -s security.json",
    "remove-airnode:local": "test -f .env && export $(egrep -v '^#' .env | xargs) && yarn remove-airnode",
    "remove-mnemonic": "node dist/deployer-cli.js remove-mnemonic -c config.json -s security.json",
    "remove-mnemonic:local": "test -f .env && export $(egrep -v '^#' .env | xargs) && yarn remove-mnemonic",
    "terraform:init": "cd terraform && terraform init",
    "sls:config": "sls config credentials --provider aws --key $AWS_ACCESS_KEY_ID --secret $AWS_SECRET_KEY",
    "sls:config:local": "test -f .env && export $(egrep -v '^#' .env | xargs) && yarn sls:config",
    "sls:deploy": "sls deploy --config serverless.aws.yml --providerIdShort $PROVIDER_ID_SHORT --awsRegion $AWS_REGION",
    "sls:deploy:local": "test -f .env && export $(egrep -v '^#' .env | xargs) && yarn sls:deploy",
    "sls:remove": "sls remove --config serverless.aws.yml --providerIdShort $PROVIDER_ID_SHORT --awsRegion $AWS_REGION",
    "sls:remove:local": "test -f .env && export $(egrep -v '^#' .env | xargs) && yarn sls:remove",
    "sls:invoke:local": "test -f .env && export $(egrep -v '^#' .env | xargs) && sls invoke --function start --config serverless.aws.yml --providerIdShort 9e5a89d --awsRegion $AWS_REGION",
    "sls:package:local": "test -f .env && export $(egrep -v '^#' .env | xargs) && sls package --config serverless.aws.yml --providerIdShort 9e5a89d --awsRegion $AWS_REGION"
  },
  "dependencies": {
    "@airnode/node": "^0.1.0",
    "@types/yargs": "^15.0.9",
    "ethers": "^5.0.17",
    "ora": "^5.1.0",
    "serverless": "^2.8.0",
    "serverless-plugin-optimize": "^4.1.4-rc.1",
    "serverless-plugin-typescript": "^1.1.9",
    "yargs": "^16.0.3"
  }
}
