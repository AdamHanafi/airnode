{
  "name": "@airnode/deployer",
  "version": "0.1.0",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "deploy": "node .build/deployer-cli.js deploy -c config.json -s security.json",
    "help": "node .build/deployer-cli.js --help",
    "remove-airnode": "node .build/deployer-cli.js remove-airnode -c config.json -s security.json",
    "remove-mnemonic": "node .build/deployer-cli.js remove-mnemonic -c config.json -s security.json",
    "terraform:init": "cd terraform && terraform init",
    "sls:invoke:aws": "sls invoke --function start --config serverless.aws.yml",
    "sls:test:aws": "sls invoke --function test --config serverless.aws.yml"
  },
  "dependencies": {
    "@airnode/node": "^0.1.0",
    "ethers": "^5.0.17",
    "ora": "^5.1.0",
    "serverless-plugin-typescript": "^1.1.9",
    "yargs": "^16.0.3"
  }
}
